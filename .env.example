# Development Environment Variables
# Copy this file to .env and fill in your actual values

# Development Database (required)
DATABASE_URL=postgresql://username:password@localhost:5432/aquaflow_dev
SESSION_SECRET=your-development-session-secret-key-here

# Production Environment Variables (optional for development)
# These are used when NODE_ENV=production
PRODUCTION_DATABASE_URL=postgresql://username:password@production-host:5432/aquaflow_prod
PRODUCTION_SESSION_SECRET=your-production-session-secret-key-here

# Optional: Customize connection pool settings
# DB_POOL_MIN=1
# DB_POOL_MAX=5
# DB_POOL_IDLE_TIMEOUT=15000

# Application Settings
NODE_ENV=development
PORT=5000